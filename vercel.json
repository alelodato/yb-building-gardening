{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "ybpropertyservices.com" }],
      "destination": "https://www.ybpropertyservices.com/$1",
      "permanent": true
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}